<section>
  <table class="table">
    <thead>
      <tr>
        <th class="col">Nombres</th>
        <th class="col">Clave</th>
        <th class="col">Facultad</th>
        <th class="col">Creado por</th>
        <th class="col">Activo</th>
        <th class="col">Opciones</th>
      </tr>
    </thead>
    <tbody>
      {{#data}}
        <tr data-id={{cod_usuario}}>
          <td scope="row">{{nombre}}</td>
          <td>{{clave}}</td>
          <td>{{cod_fac}}</td>
          <td>{{usuario_creador}}</td>
          <td>{{activo}}</td>
          <td>
            <button id="edit" class="btn btn-primary">Editar</button>
          </td>
        </tr>
      {{/data}}
    </tbody>
  </table>
</section>