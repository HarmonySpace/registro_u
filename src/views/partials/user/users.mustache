<section>
	<table class="table">
		<thead>
			<tr>
				<th class="col">ID</th>
				<th class="col">Nombre</th>
				<th class="col">Apellidos</th>
				<th class="col">Registrado</th>
				<th class="col">Menu</th>
				<th class="col">Cargo</th>
				<th class="col">Activo</th>
				<th class="col">Eliminado</th>
				<th class="col">Opciones</th>
			</tr>
		</thead>
		<tbody>
			{{# data }}
			<tr data-id={{id_usuario}}>
				<td>{{id_usuario}}</td>
				<td>{{nombre}}</td>
				<td>{{apellidos}}</td>
				<td>{{fecha_creacion}}</td>
				<td>{{id_menu}}</td>
				<td>{{cargo}}</td>
				<td>{{activo}}</td>
				<td>{{eliminado}}</td>
				<td>
					<button id="edit" class="btn btn-primary">Editar</button>
				</td>
			</tr>
			{{/ data }}
		</tbody>
	</table>
</section>
