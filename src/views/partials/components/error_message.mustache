<div id="liveToast" class="toast align-items-center text-bg-primary bg-danger border-0 error-message" role="alert" aria-live="assertive"
	aria-atomic="true">
	<div class="d-flex">
		<div class="toast-body">
			<span class="bold">ERROR:</span> {{{message}}}
		</div>
		<button type="button" class="btn-close btn-close-white me-2 m-auto" data-bs-dismiss="toast"
			aria-label="Close"></button>
	</div>
</div>